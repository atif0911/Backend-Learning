<body>
<%- include('partials/navbar') %>
    <main>
        <h1>Hello from Airbnb</h1>
        <a href="/host/add-home">Add Home</a>
        <h2>Available Homes</h2>
        <ul>
            <% registeredHomes.forEach(home=>{%>
                <li>
                    <h3>Home Name: <%=home.houseName%></h3>
                    <p>Address: <%=home.address%></p>
                </li>
            <% })%>
        </ul>
    </main>
    
</body>
</html>