<%- include('../partials/head') %>
</head>
<body>
  <%- include('../partials/navbar') %>

  <main class="home-details">
    <h1 class="details-title">🏡 Home Details</h1>
    <div class="home-card">
      <img src="<%=home.photo%>" alt="Home Image" class="home-image">

      <div class="home-info">
        <h3 class="home-name"><%=home.houseName%></h3>
        <p class="home-price">💵 Price per Night: <span>$<%=home.pricePerNight%></span></p>
        <p class="home-location">📍 Location: <%=home.location%></p>
        <p class="home-rating">⭐ Rating: <%=home.rating%></p>
        <p class="home-location"> Description: <%=home.description%></p>
        <%- include('../partials/favourite')%>
      </div>
    </div>
  </main>
</body>
</html>
